{
  "name": "headrest",
  "author": "Nat Lownes <nat.lownes@gmail.com>",
  "description": "A restful, CORS enabled development server - for clicking around front end Javascript projects.",
  "main": "./lib/index.js",
  "version": "0.1.2",
  "dependencies": {
    "express": "~3.1.0",
    "whoadb": "~0.1.x",
    "headrest-middleware": ">=0.0.5",
    "moment": "~2.0.x"
  },
  "devDependencies": {
    "coffee-script": "~1.6.0",
    "honk-cake": ">=0.0.5",
    "grunt": "~0.4.0",
    "mocha": "~1.8.1",
    "superagent": "~0.14.x",
    "chai": "~1.4.2",
    "q": "~0.9.x",
    "coffeelint": "~1.1.0"
  },
  "directories": {
    "test": "test",
    "src": "src",
    "lib": "lib"
  },
  "scripts": {
    "test": "npm run lint && cake test",
    "test:watch": "cake test:watch",
    "build": "find ./src -iname '*.coffee' | xargs coffee -c -o lib",
    "lint": "find ./src ./test -iname '*.coffee' | xargs coffeelint",
    "push": "git push origin master && git push origin --tags",
    "prepublish": "npm run build"
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git://github.com/natlownes/headrest.git"
  },
  "keywords": [
    "REST",
    "api",
    "webserver",
    "server",
    "http",
    "test",
    "testing",
    "js",
    "frontend",
    "ui",
    "clicking"
  ],
  "license": "BSD",
  "optionalDependencies": {},
  "engines": {
    "node": ">= 0.8"
  }
}
